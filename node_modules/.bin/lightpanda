#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/dist/cli/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/dist/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/dist/cli/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/dist/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/browser/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules/@lightpanda/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/@lightpanda+browser@1.0.1/node_modules:/Users/nehuen/LeadSwap/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@lightpanda/browser/dist/cli/main.js" "$@"
else
  exec node  "$basedir/../@lightpanda/browser/dist/cli/main.js" "$@"
fi
