  "scripts": {
    "dev": "pnpm prisma:generate && next dev --port 3001",
    "build": "pnpm prisma:generate && next build",
    "typecheck": "tsc --noEmit",
    "lint": "next lint",
    "start": "next start",
    "test": "pnpm prisma:generate && vitest --no-file-parallelism --bail=1",
    "prisma:generate": "dotenv-flow -e .env -- pnpm --filter=@leadswap/prisma generate",
    "prisma:push": "dotenv-flow -e .env -- pnpm --filter=@leadswap/prisma push",
    "prisma:studio": "dotenv-flow -e .env -- pnpm --filter=@leadswap/prisma studio"
  },
